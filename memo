# 버전 체크 무시 + 외부 접속 허용(0.0.0.0) 옵션 포함
DISABLE_VERSION_CHECK=1 GRADIO_SERVER_NAME=0.0.0.0 llamafactory-cli webui

# 버전 체크를 무시하고 실행
DISABLE_VERSION_CHECK=1 llamafactory-cli export \
    --model_name_or_path Qwen/Qwen3-VL-2B-Instruct \
    --adapter_name_or_path . \
    --template qwen2_vl \
    --export_dir ./merged_qwen3_model \
    --export_device cpu \
    --export_legacy_format false

cd ..
tensorrt-edgellm-quantize-llm \
    --model_dir ./merged_qwen3_model \
    --output_dir ./quantized/qwen3-vl-2b-int4 \
    --quantization int4_awq

tensorrt-edgellm-export-llm \
    --model_dir ./quantized/qwen3-vl-2b-int4 \
    --output_dir onnx_models/qwen3-vl-2b-int4

tensorrt-edgellm-export-visual \
    --model_dir ./merged_qwen3_model \
    --output_dir onnx_models/qwen3-vl-2b-int4/visual_enc_onnx



~~~~~~~

tensorrt-edgellm-quantize-llm \
    --model_dir Qwen/Qwen3-4B \
    --output_dir ./quantized/qwen3-4b \
    --quantization int4_awq
